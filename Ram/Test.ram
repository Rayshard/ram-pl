def Student = {	name : string; age : int; school : string; pet : Animal };

func CallNum() : int =
{
	let cnt = 0;
	continue;
	for i = 0 if i < 10 then
	{
		cnt = cnt + 2;
	} finally i=i+1;

	return cnt;
};

