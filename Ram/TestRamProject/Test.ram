include("Includes.ram", "Includes", false);
include("..\\RamLibraries\\ram_time.ram", "RamTime", true);

let startTime = Time();

func Factorial(num:int) : int = {
  func helper(num:int, acc:int) : int =
    if(num <= 0) then acc
    else helper(num - 1, acc * num);

  return helper(num, 1);
};

let student = {
  name = "ray";
  age = 21;
  school = "Cornell University";
  pet = { name = "Sidney"; alive = true }
};

let dog = student.pet;

unscope(Includes);
PrintStudent(student);
PrintStudent(student);
PrintAnimal(dog);

let facVal = Factorial(5);
print(<string>(facVal--));
print("\n");

let endTime = Time();
print("This program ran in " + GetElapsedTime(startTime, endTime, "s") + " seconds!\n");
